{# templates/objets_connectes/add.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Ajouter un objet connecté{% endblock %}

{% block body %}
    <h1>Ajouter un objet connecté</h1>

    {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.type) }}
        {{ form_row(form.marque) }}
        {{ form_row(form.etat) }}
        {{ form_row(form.consommationEnergetique) }}
        <!-- Ajoutez d'autres champs pour les objets ici -->
        <button class="btn btn-primary">Ajouter</button>
    {{ form_end(form) }}
{% endblock %}
