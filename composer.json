{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/cache": "^2.2",
        "doctrine/collections": "^2.3",
        "doctrine/data-fixtures": "^2.0",
        "doctrine/dbal": "^4.2",
        "doctrine/deprecations": "^1.1",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/event-manager": "^2.0",
        "doctrine/inflector": "^2.0",
        "doctrine/instantiator": "^2.0",
        "doctrine/lexer": "^3.0",
        "doctrine/migrations": "^3.8",
        "doctrine/orm": "^3.3",
        "doctrine/persistence": "^3.4",
        "doctrine/sql-formatter": "^1.5",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/asset": "7.2.*",
        "symfony/cache": "7.2.*",
        "symfony/cache-contracts": "^3.5",
        "symfony/config": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/dependency-injection": "7.2.*",
        "symfony/deprecation-contracts": "^3.5",
        "symfony/doctrine-bridge": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/error-handler": "7.2.*",
        "symfony/event-dispatcher": "7.2.*",
        "symfony/event-dispatcher-contracts": "^3.5",
        "symfony/filesystem": "7.2.*",
        "symfony/finder": "7.2.*",
        "symfony/flex": "^2",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-foundation": "7.2.*",
        "symfony/http-kernel": "7.2.*",
        "symfony/maker-bundle": "^1.62",
        "symfony/options-resolver": "7.2.*",
        "symfony/password-hasher": "7.2.*",
        "symfony/polyfill-intl-icu": "^1.31",
        "symfony/polyfill-intl-normalizer": "^1.31",
        "symfony/polyfill-mbstring": "^1.31",
        "symfony/polyfill-php83": "^1.31",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/routing": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/security-core": "7.2.*",
        "symfony/service-contracts": "^3.5",
        "symfony/stopwatch": "7.2.*",
        "symfony/translation-contracts": "^3.5",
        "symfony/twig-bridge": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/var-dumper": "7.2.*",
        "symfony/var-exporter": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.20"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
